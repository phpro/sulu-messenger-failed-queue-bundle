{
    "name": "tailrdigital/sulu-messenger-failed-queue-bundle",
    "keywords": ["sulu", "messenger", "failed-queue"],
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Tailr\\SuluMessengerFailedQueueBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tailr\\SuluMessengerFailedQueueBundle\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "~8.1.0 || ~8.2.0 || ~8.3.0",
        "azjezz/psl": "^2.6 || ^3.0",
        "doctrine/dbal": "^3.0",
        "doctrine/orm": "^2.14",
        "sulu/sulu": "^2.5",
        "jackalope/jackalope-doctrine-dbal": "^1.6 || ^2.0",
        "symfony/config": "^6.0",
        "symfony/console": "^6.0",
        "symfony/dependency-injection": "^6.0",
        "symfony/http-foundation": "^6.0",
        "symfony/doctrine-messenger": "^6.0",
        "symfony/messenger": "^6.0"
    },
    "require-dev": {
        "php-cs-fixer/shim": "^3.41",
        "phpro/symfony-conventions": "^1.5",
        "phpspec/prophecy-phpunit": "^2.1",
        "phpunit/phpunit": "^9.6",
        "psalm/plugin-symfony": "^5.1",
        "vimeo/psalm": "^5.18",
        "weirdan/doctrine-psalm-plugin": "^2.9"
    },
    "config": {
        "allow-plugins": {
            "phpro/grumphp-shim": true,
            "phpstan/extension-installer": true,
            "php-http/discovery": true
        },
        "sort-packages": true
    }
}
